    "@nestjs/mongoose": "^3.0.1",
    "@nestjs/passport": "^1.0.10",
    "@nestjs/testing": "^5.0.0",
    "@nestjs/websockets": "^5.0.0",
    "bcrypt": "^2.0.1",
    "class-transformer": "^0.1.7",
    "class-validator": "^0.8.1",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.1.0",
    "mongoose-unique-validator": "^2.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typescript": "^2.8.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/mongoose": "^5.0.13",
    "@types/node": "^7.0.41",
    "jest": "^20.0.4",
    "supertest": "^3.0.0",
    "ts-jest": "^20.0.14",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1"
